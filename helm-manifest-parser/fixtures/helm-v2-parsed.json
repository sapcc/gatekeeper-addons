{
  "items": [
    {
      "apiVersion": "apiextensions.k8s.io/v1beta1",
      "kind": "CustomResourceDefinition",
      "metadata": {
        "name": "maintenances.admin-k8s.sap.cc"
      },
      "spec": {
        "group": "admin-k8s.sap.cc",
        "names": {
          "kind": "Maintenance",
          "plural": "maintenances",
          "shortNames": [
            "mt"
          ],
          "singular": "maintenance"
        },
        "scope": "Cluster",
        "version": "v1"
      }
    },
    {
      "apiVersion": "extensions/v1beta1",
      "kind": "Deployment",
      "metadata": {
        "name": "admin-k8s",
        "namespace": "kube-system"
      },
      "spec": {
        "replicas": 1,
        "revisisonHistoryLimit": 5,
        "selector": {
          "matchLabels": {
            "app": "admin-k8s"
          }
        },
        "template": {
          "metadata": {
            "labels": {
              "app": "admin-k8s"
            }
          },
          "spec": {
            "containers": [
              {
                "image": "hub.global.cloud.sap/monsoon/admin-k8s:20190108095901",
                "name": "admin-k8s"
              }
            ],
            "livenessProbe": {
              "httpGet": {
                "path": "/system/liveliness",
                "port": 80
              },
              "initialDelaySeconds": 60,
              "periodSeconds": 60,
              "timeoutSeconds": 10
            },
            "ports": [
              {
                "containerPort": 80,
                "name": "web"
              }
            ],
            "readinessProbe": {
              "httpGet": {
                "path": "/system/readiness",
                "port": 80
              },
              "periodSeconds": 5,
              "timeoutSeconds": 5
            }
          }
        }
      }
    }
  ]
}
